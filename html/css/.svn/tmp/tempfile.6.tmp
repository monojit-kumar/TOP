.discount p{ font-size:12px; font-weight:normal; color:#55656f;}
.code{ border:1px solid #d8d8d8; width:970px; margin:20px auto; background:#f6f6f6;}
.code_table{ width:100%;}
.code_table tr.code_table_head{ background:#e4e4e4; height:33px;}
.code_table tr.sndRow{ min-height:25px;}
.code_table tr td{ padding:0 0 0 20px; font-size:13px; color:#1e2a33; vertical-align:middle;}
.coupndts{ padding:0 0 0 23px; line-height:40px;}
.txtcenter{ text-align:center;}
.discell{ width:40px;}
.dscnt{ width:270px;}
.discountBtn{
	width:112px; height:28px;
	line-height:28px; text-align:center;
	cursor:pointer;border:1px solid #cecece;
	font-size:12px; color:#606e78;
	-webkit-border-radius: 6px;
	-moz-border-radius: 6px;
	border-radius: 6px;
	background: #C7C7C7;
	background: -webkit-gradient(linear, 0 0, 0 bottom, from(#C7C7C7), to(#ebebeb));
	background: -webkit-linear-gradient(#C7C7C7, #ebebeb);
	background: -moz-linear-gradient(#C7C7C7, #ebebeb);
	background: -ms-linear-gradient(#C7C7C7, #ebebeb);
	background: -o-linear-gradient(#C7C7C7, #ebebeb);
	background: linear-gradient(#C7C7C7, #ebebeb);
	-pie-background: linear-gradient(#C7C7C7, #ebebeb);
	behavior: url(PIE.htc);
	position:relative;
}
.discountBtn:hover{background: #C7C7C7;}
.codeCell{ width:330px;}
.dsblBtn{ width:113px;}
.blnk{ width:50px;}
.codeName{
	width:112px; height:28px;
	line-height:28px; text-align:center;
	cursor:pointer; margin: 12px 0;
	border:none; font-weight:bold;
	font-size:12px; color:#13415c;
	-webkit-border-radius: 6px;
	-moz-border-radius: 6px;
	border-radius: 6px;
	-webkit-box-shadow: 2px 1px 1px #bfbfbf;
	-moz-box-shadow: 2px 1px 1px #bfbfbf;
	box-shadow: 2px 1px 1px #bfbfbf;
	background: #e5e5e5;
	behavior: url(PIE.htc);
	position:relative;
}
.codeName:hover{ background:#d7d5d5;}
.iconArea{ 
	line-height: 18px;
    position: absolute;
    right: 46px;
    top: 1px;
}
.editicon{ padding:5px 1px; display:block; float:left;}